{
  "name": "create-solana-dapp",
  "version": "4.2.9",
  "description": "Get up and running fast with Solana dApps",
  "repository": {
    "name": "solana-developers/create-solana-dapp",
    "type": "git",
    "url": "https://github.com/solana-developers/create-solana-dapp"
  },
  "homepage": "https://github.com/solana-developers/create-solana-dapp#readme",
  "bugs": {
    "url": "https://github.com/solana-developers/create-solana-dapp/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-solana-dapp": "./dist/bin/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:watch": "unbuild --watch",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm build && pnpm test && npm publish --tag latest && git push --follow-tags",
    "release:next": "pnpm build && pnpm test && npm publish --tag next && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@types/jest": "^29.5.14",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/semver": "^7.7.0",
    "@vitest/coverage-v8": "^3.2.2",
    "automd": "^0.3.12",
    "eslint": "^9.28.0",
    "eslint-config-unjs": "^0.4.2",
    "jest": "29.7.0",
    "mock-fs": "^5.5.0",
    "prettier": "^3.5.3",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "29.3.4",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.2"
  },
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@project-serum/anchor": "^0.26.0",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/wallet-adapter-phantom": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.38",
    "@solana/wallet-adapter-react-ui": "^0.9.38",
    "@solana/wallet-adapter-solflare": "^0.6.31",
    "@solana/wallet-adapter-wallets": "^0.19.36",
    "@solana/web3.js": "^1.98.2",
    "commander": "^13.1.0",
    "core-js": "^3.42.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "eslint-define-config": "^2.1.0",
    "fr": "^1.2.2",
    "giget": "^1.2.5",
    "next": "^15.3.3",
    "picocolors": "^1.1.1",
    "pnpm": "^10.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.7.2",
    "std-env": "^3.9.0",
    "typedef": "^1.1.0",
    "up": "^1.0.2",
    "zod": "^3.25.56"
  },
  "contributors": [
    {
      "name": "Joe Caulfield",
      "url": "https://github.com/jpcaulfi"
    },
    {
      "name": "Jacob Creech",
      "url": "https://github.com/jacobcreech"
    },
    {
      "name": "Nick Frostbutter",
      "url": "https://github.com/nickfrosty"
    },
    {
      "name": "Bram Borggreve",
      "url": "https://github.com/beeman"
    }
  ],
  "keywords": [
    "solana",
    "next",
    "react",
    "web3",
    "blockchain",
    "nft"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "moduleResolution": "node",
      "types": [
        "node",
        "next"
      ]
    }
  }
}
