{
  "name": "template-next-tailwind",
  "description": "Next.js, Tailwind, gill (based on @solana/kit), Wallet UI",
  "keywords": [
    "gill",
    "nextjs",
    "react",
    "solana-kit",
    "tailwind",
    "typescript",
    "wallet-ui"
  ],
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next dist out",
    "anchor:build": "cd anchor && cargo clean && cargo update && anchor build && cd .. && cp -r anchor/target/idl/*  target/ && cp -r anchor/target/types/* target/ && next build",
    "anchor:deploy": "cd anchor && anchor deploy --provider.cluster devnet && cd ..",
    "anchor:test": "cd anchor && anchor test && cd ..",
    "anchor:clean": "rm -rf anchor/target && anchor build",
    "solana:airdrop": "solana airdrop 2",
    "solana:key": "solana-keygen new --outfile ~/.config/solana/id.json",
    "solana:balance": "solana balance",
    "solana:config": "solana config get",
    "docker:build": "docker build -t anaheim:latest .",
    "docker:tag": "docker tag anaheim:latest anarcrypt/anaheim:latest",
    "docker:push": "docker push anarchsun/anaheim:latest",
    "docker:run": "docker run -p 3000:3000 anaheim:latest",
    "scaffold:init": "ts-node ./scripts/init-template.ts",
    "scaffold:fetch": "ts-node ./scripts/fetch-templates.ts",
    "putsch:sync": "pnpm anchor:build && pnpm build && pnpm lint",
    "putsch:dev": "pnpm solana:airdrop && pnpm dev",
    "putsch:deploy": "pnpm anchor:deploy && echo 'Deployed on devnet!'"
  },
  "dependencies": {
    "@anza-xyz/solana-rpc-get-stake-activation": "^1.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/web3.js": "^1.98.2",
    "@tanstack/react-query": "^5.80.7",
    "@wallet-ui/react": "1.1.0-canary-20250614160139",
    "@wallet-ui/tailwind": "1.1.0-canary-20250614160139",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "formatDistanceToNowStrict": "link:@/date-fns@4.1.0/node_modules/date-fns/formatDistanceToNowStrict",
    "gill": "0.10.1",
    "gill-react": "^0.4.3",
    "jotai": "^2.12.5",
    "lucide-react": "^0.515.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "sonner": "^2.0.5",
    "styles.css": "link:@solana/wallet-adapter-react-ui/styles.css",
    "superstruct": "^2.0.2",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "ws": "^8.18.2"
  }
}
